{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "api/index.py"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "memory": 1024,
      "maxDuration": 30,
      "excludeFiles": "{chroma_db/**,data/**,tests/**,api/tests/**,scripts/**,public/models/**,node_modules/**,.next/**}"
    }
  }
}
